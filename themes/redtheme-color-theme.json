{
  "name": "redtheme",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#19191a",
    "editor.foreground": "#ff0000",
    "editor.lineHighlightBackground": "#202020",
    "activityBarBadge.background": "#ff0000",
    "sideBarTitle.foreground": "#ff0000",

    /* Status Bar */
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"

    /* Brackets */
    // "editorBracketHighlight.foreground1": "#ff0000",
    // "editorBracketHighlight.foreground2": "#ff0000",
    // "editorBracketHighlight.foreground3": "#ff0000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#53545e"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#f3dbb0"
      }
    },
    {
      "name": "Variables, Functions, Classes",
      "scope": ["variable", "entity.name.function", "entity.name.type.class"],
      "settings": {
        "foreground": "#d2d9df"
      }
    },
    // {
    //   "name": "Variables, Functions, Classes",
    //   "scope": ["entity.name.function", "entity.name.type.class"],
    //   "settings": {
    //     "foreground": "#ee99cd"
    //   }
    // },
    {
      "name": "Assignment",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e2a967"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#53545e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings, Numbers, Quotations, Null",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.numeric",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#6090e9"
        // "foreground": "#4bd173"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.definition.block",
        "punctuation.definition.binding-pattern.object",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#53545e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#d6caba"
      }
    }
  ]
}
